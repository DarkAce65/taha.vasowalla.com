{
    "name": "taha.vasowalla.com",
    "private": true,
    "sideEffects": false,
    "ignore": [
        "**/.*",
        "node_modules",
        "bower_components",
        "test",
        "tests"
    ],
    "scripts": {
        "eslint:fix": "eslint --fix \"gulpfile.js\" \"*.config.js\" \"gulp/**/*.js\" \"src/**/*.{js,ts}\"",
        "prettier:write": "prettier --write \"gulpfile.js\" \".eslintrc.js\" \"*.config.js\" \"tsconfig.json\" \"gulp/**/*.js\" \"src/**/*.{js,ts,scss}\"",
        "copy:static": "gulp copy:static --silent",
        "dev": "gulp dev --silent",
        "build": "gulp --silent",
        "build:prod": "cross-env NODE_ENV=production PUBLIC_PATH='/' gulp --silent"
    },
    "husky": {
        "hooks": {
            "pre-commit": "lint-staged"
        }
    },
    "lint-staged": {
        "*.{js,ts}!eslintrc.js": "eslint --max-warnings=0",
        "*.{js,ts,scss}": "prettier --list-different"
    },
    "devDependencies": {
        "@babel/core": "^7.12.17",
        "@babel/plugin-transform-strict-mode": "^7.12.13",
        "@babel/preset-env": "^7.12.17",
        "@types/gsap": "^1.20.2",
        "@types/js-cookie": "^2.2.6",
        "@types/katex": "^0.11.0",
        "@types/uikit": "^3.3.1",
        "@types/wavesurfer.js": "^3.3.1",
        "@types/webpack": "^4.41.26",
        "@typescript-eslint/eslint-plugin": "^4.15.1",
        "@typescript-eslint/parser": "^4.15.1",
        "babel-eslint": "^10.1.0",
        "babel-loader": "^8.2.2",
        "chalk": "^4.1.0",
        "cross-env": "^7.0.3",
        "css-loader": "^5.0.2",
        "del": "^6.0.0",
        "eslint": "^7.20.0",
        "eslint-config-prettier": "^8.0.0",
        "eslint-plugin-import": "^2.22.1",
        "eslint-plugin-prettier": "^3.3.1",
        "eslint-webpack-plugin": "^2.5.2",
        "extract-loader": "^5.1.0",
        "fancy-log": "^1.3.3",
        "file-loader": "^6.2.0",
        "gulp": "^4.0.2",
        "gulp-autoprefixer": "^7.0.1",
        "gulp-sass": "^4.1.0",
        "gulp-using": "^0.1.1",
        "html-webpack-plugin": "^5.2.0",
        "husky": "^5.1.0",
        "lint-staged": "^10.5.4",
        "prettier": "^2.2.1",
        "pug": "^3.0.0",
        "pug-loader": "^2.4.0",
        "raw-loader": "^4.0.2",
        "sass": "^1.32.8",
        "sass-graph": "^3.0.5",
        "serve": "^11.3.2",
        "ts-loader": "^8.0.17",
        "tsconfig-paths-webpack-plugin": "^3.3.0",
        "typescript": "^4.1.5",
        "webpack": "^5.23.0",
        "webpack-dev-server": "^4.0.0-beta.0",
        "webpack-stream": "^6.1.2"
    },
    "dependencies": {
        "@fortawesome/fontawesome-svg-core": "^1.2.34",
        "@fortawesome/free-regular-svg-icons": "^5.15.2",
        "@fortawesome/free-solid-svg-icons": "^5.15.2",
        "buffer": "^6.0.3",
        "core-js": "^3.9.0",
        "gsap": "^3.6.0",
        "js-cookie": "^2.2.1",
        "katex": "^0.12.0",
        "music-metadata-browser": "^2.2.5",
        "process": "^0.11.10",
        "simplex-noise": "^2.4.0",
        "three": "^0.125.2",
