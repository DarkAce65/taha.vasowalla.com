{
    "name": "taha.vasowalla.com",
    "private": true,
    "ignore": [
        "**/.*",
        "node_modules",
        "bower_components",
        "test",
        "tests"
    ],
    "scripts": {
        "prettier:write": "prettier --write \"gulpfile.js\" \".eslintrc.js\" \"*.config.js\" \"gulp/**/*.js\" \"src/**/*.{js,scss}\"",
        "copy:static": "gulp copy:static --silent",
        "build": "NODE_ENV=production gulp --silent",
        "dev": "gulp --silent",
        "watch": "gulp watch --silent"
    },
    "husky": {
        "hooks": {
            "pre-commit": "lint-staged"
        }
    },
    "lint-staged": {
        "*.js": "eslint --max-warnings=0",
        "*.{js,scss}": "prettier --list-different"
    },
    "devDependencies": {
        "@babel/core": "^7.6.2",
        "@babel/plugin-transform-strict-mode": "^7.2.0",
        "@babel/preset-env": "^7.6.2",
        "babel-eslint": "^10.0.3",
        "babel-loader": "^8.0.6",
        "css-loader": "^3.2.0",
        "del": "^5.1.0",
        "eslint": "^6.4.0",
        "eslint-config-prettier": "^6.3.0",
        "eslint-loader": "^3.0.1",
        "eslint-plugin-prettier": "^3.1.1",
        "extract-loader": "^3.1.0",
        "file-loader": "^4.2.0",
        "gulp": "^4.0.2",
        "gulp-autoprefixer": "^7.0.0",
        "gulp-if": "^3.0.0",
        "gulp-sass": "^4.0.2",
        "gulp-using": "^0.1.1",
        "html-webpack-plugin": "^4.0.0-beta.5",
        "husky": "^3.0.5",
        "lint-staged": "^9.4.0",
        "prettier": "^1.18.2",
        "pug": "^2.0.4",
        "pug-loader": "^2.4.0",
        "raw-loader": "^3.1.0",
        "sass": "^1.22.12",
        "sass-graph": "^3.0.4",
        "serve": "^11.2.0",
        "through2": "^3.0.1",
        "webpack": "^4.41.0",
        "webpack-stream": "^5.2.1"
    },
    "dependencies": {
        "core-js": "^3.2.1",
        "gsap": "^2.1.3",
        "simplex-noise": "^2.4.0",
        "three": "^0.108.0",
        "uikit": "^3.2.0"
    }
}
