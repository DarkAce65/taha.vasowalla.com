{
    "name": "taha.vasowalla.com",
    "private": true,
    "sideEffects": false,
    "ignore": [
        "**/.*",
        "node_modules",
        "bower_components",
        "test",
        "tests"
    ],
    "scripts": {
        "postinstall": "patch-package",
        "prepare": "husky install",
        "eslint:fix": "eslint --fix \"gulpfile.js\" \"*.config.js\" \"gulp/**/*.js\" \"src/**/*.{js,ts}\"",
        "prettier:write": "prettier --write \"gulpfile.js\" \".eslintrc.js\" \"*.config.js\" \"tsconfig.json\" \"gulp/**/*.js\" \"src/**/*.{pug,js,ts,scss}\"",
        "copy:static": "gulp copy:static --silent",
        "dev": "gulp dev --silent",
        "build": "gulp --silent",
        "build:prod": "cross-env NODE_ENV=production PUBLIC_PATH='/' gulp --silent"
    },
    "lint-staged": {
        "*.{js,ts}!eslintrc.js": "eslint --max-warnings=0",
        "*.{pug,js,ts,scss}": "prettier --list-different"
    },
    "devDependencies": {
        "@babel/core": "^7.14.8",
        "@babel/eslint-parser": "^7.14.7",
        "@babel/plugin-transform-strict-mode": "^7.14.5",
        "@babel/preset-env": "^7.14.8",
        "@mr-hope/gulp-sass": "^2.0.0",
        "@prettier/plugin-pug": "^1.16.1",
        "@types/chroma-js": "^2.1.3",
        "@types/js-cookie": "^2.2.7",
        "@types/katex": "^0.11.1",
        "@types/p2": "^0.7.37",
        "@types/three": "^0.130.1",
        "@types/uikit": "^3.3.2",
        "@types/wavesurfer.js": "^5.1.0",
        "@types/webpack": "^5.28.0",
        "@typescript-eslint/eslint-plugin": "^4.28.5",
        "@typescript-eslint/parser": "^4.28.5",
        "babel-loader": "^8.2.2",
        "chalk": "^4.1.1",
        "cross-env": "^7.0.3",
        "css-loader": "^6.2.0",
        "del": "^6.0.0",
        "eslint": "^7.31.0",
        "eslint-config-prettier": "^8.3.0",
        "eslint-plugin-import": "^2.23.4",
        "eslint-plugin-prettier": "^3.4.0",
        "eslint-webpack-plugin": "^3.0.1",
        "fancy-log": "^1.3.3",
        "file-loader": "^6.2.0",
        "gulp": "^4.0.2",
        "gulp-autoprefixer": "^8.0.0",
        "gulp-using": "^0.1.1",
        "html-webpack-plugin": "^5.3.2",
        "husky": "^7.0.1",
        "lint-staged": "^11.1.1",
        "mini-css-extract-plugin": "^2.1.0",
        "patch-package": "^6.4.7",
        "postinstall-postinstall": "^2.1.0",
        "prettier": "^2.3.2",
        "pug": "^3.0.2",
        "pug-loader": "^2.4.0",
        "raw-loader": "^4.0.2",
        "sass-graph": "^3.0.5",
        "ts-loader": "^9.2.4",
        "tsconfig-paths-webpack-plugin": "^3.5.1",
        "typescript": "^4.3.5",
        "webpack": "^5.47.0",
        "webpack-dev-server": "^4.0.0-rc.0",
        "webpack-stream": "^6.1.2"
    },
    "dependencies": {
        "@fortawesome/fontawesome-svg-core": "^1.2.35",
        "@fortawesome/free-regular-svg-icons": "^5.15.3",
        "@fortawesome/free-solid-svg-icons": "^5.15.3",
        "buffer": "^6.0.3",
        "chroma-js": "^2.1.2",
        "core-js": "^3.15.2",
        "gsap": "^3.7.1",
