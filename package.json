{
  "name": "taha.vasowalla.com",
  "private": true,
  "sideEffects": false,
  "ignore": [
    "**/.*",
    "node_modules",
    "bower_components",
    "test",
    "tests"
  ],
  "scripts": {
    "postinstall": "patch-package",
    "prepare": "husky install",
    "eslint:fix": "eslint --fix \"*.config.{js,ts}\" \"src/**/*.{js,ts}\"",
    "prettier:write": "prettier --write \".eslintrc.js\" \"*.config.{js,ts}\" \"tsconfig.json\" \"src/**/*.{html,json,js,ts,scss}\"",
    "copy:static": "gulp copy:static --silent",
    "dev": "vite",
    "preview": "vite preview",
    "build": "tsc && vite build",
    "build:prod": "cross-env NODE_ENV=production PUBLIC_PATH='/' gulp --silent"
  },
  "lint-staged": {
    "*.{js,ts}!eslintrc.js": "eslint --max-warnings=0",
    "*.{html,json,js,ts,scss}!package.json": "prettier --list-different"
  },
