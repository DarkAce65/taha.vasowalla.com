{
    "name": "taha.vasowalla.com",
    "private": true,
    "sideEffects": false,
    "ignore": [
        "**/.*",
        "node_modules",
        "bower_components",
        "test",
        "tests"
    ],
    "scripts": {
        "eslint:fix": "eslint --fix \"gulpfile.js\" \"*.config.js\" \"gulp/**/*.js\" \"src/**/*.js\"",
        "prettier:write": "prettier --write \"gulpfile.js\" \".eslintrc.js\" \"*.config.js\" \"gulp/**/*.js\" \"src/**/*.{js,scss}\"",
        "copy:static": "gulp copy:static --silent",
        "dev": "gulp watch --silent",
        "build": "gulp --silent",
        "build:prod": "cross-env NODE_ENV=production PUBLIC_PATH='/new/' gulp --silent"
    },
    "husky": {
        "hooks": {
            "pre-commit": "lint-staged"
        }
    },
    "lint-staged": {
        "*.js!eslintrc.js": "eslint --max-warnings=0",
        "*.{js,scss}": "prettier --list-different"
    },
    "devDependencies": {
        "@babel/core": "^7.8.7",
        "@babel/plugin-transform-strict-mode": "^7.8.3",
        "@babel/preset-env": "^7.8.7",
        "babel-eslint": "^10.1.0",
        "babel-loader": "^8.0.6",
        "cross-env": "^7.0.2",
        "css-loader": "^3.4.2",
        "del": "^5.1.0",
        "eslint": "^6.8.0",
        "eslint-config-prettier": "^6.10.0",
        "eslint-loader": "^3.0.3",
        "eslint-plugin-import": "^2.20.1",
        "eslint-plugin-prettier": "^3.1.2",
        "extract-loader": "^5.0.1",
        "file-loader": "^5.1.0",
        "gulp": "^4.0.2",
        "gulp-autoprefixer": "^7.0.1",
        "gulp-if": "^3.0.0",
        "gulp-sass": "^4.0.2",
        "gulp-using": "^0.1.1",
        "html-webpack-plugin": "^4.0.0-beta.11",
        "husky": "^4.2.3",
        "lint-staged": "^10.0.8",
        "prettier": "^1.19.1",
        "pug": "^2.0.4",
        "pug-loader": "^2.4.0",
        "raw-loader": "^4.0.0",
        "sass": "^1.26.3",
        "sass-graph": "^3.0.4",
        "serve": "^11.3.0",
        "through2": "^3.0.1",
        "webpack": "^4.42.0",
        "webpack-dev-server": "^3.10.3",
        "webpack-stream": "^5.2.1"
    },
    "dependencies": {
        "@fortawesome/fontawesome-svg-core": "^1.2.27",
        "@fortawesome/free-regular-svg-icons": "^5.12.1",
        "@fortawesome/free-solid-svg-icons": "^5.12.1",
        "core-js": "^3.6.4",
        "gsap": "^3.2.4",
        "katex": "^0.11.1",
        "music-metadata-browser": "^2.0.4",
        "simplex-noise": "^2.4.0",
        "three": "^0.114.0",
        "uikit": "^3.3.6",
        "wavesurfer.js": "^3.3.1"
    }
}
